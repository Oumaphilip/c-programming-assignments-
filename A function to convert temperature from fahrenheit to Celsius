//a function to convert from fahrenheit to Celsius
#include<iostream>
using namespace std;
float FahrenheittoCelsius(float);
int main ()
{
float Fahrenheit,celsius;
cout<<"Enter the temperature in Fahrenheit:";
cin>>fahrenheit;
celsius=FahrenheittoCelsius(float);
cout<<endlfahrenheit<<"\370F="<<celsius<<\370C";
